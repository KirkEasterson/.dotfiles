# unbind keys
unbind-key h
unbind-key j
unbind-key k
unbind-key l
unbind-key h
unbind-key J
unbind-key K
unbind-key L

unbind-key ENTER
unbind-key g
unbind-key G
unbind-key u
unbind-key d
unbind-key a
unbind-key n
unbind-key p
unbind-key N
unbind-key P

# bind keys - vim style
bind-key h quit
bind-key j down
bind-key k up
bind-key l open
bind-key H quit
bind-key J down
bind-key K up
bind-key L open

bind-key g home
bind-key G end
bind-key u pageup
bind-key d pagedown
bind-key a toggle-article-read
bind-key n next-unread
bind-key p prev-unread
bind-key N next-unread
bind-key P prev-unread
